{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "CVS_DB_CONNECTION_STRING"
  },
  "InternalAuthKey": "CVS_JWT_SECRET",
  "JwtSettings": {
    "AccessTokenExpiration": "00:30:00", // 30 minutes for access token
    "RefreshTokenExpiration": "30.00:00:00" // 30 days for refresh token
  },
  "AllowedHosts": "*"
}
